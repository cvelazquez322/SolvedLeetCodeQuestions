# Given two arrays, write a function to compute their intersection.




class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        if len(nums1) == 0 or len(nums2) == 0:
            return []
        rlist = []
        for x in nums1:
            if x in nums2:
                rlist.append(x)
                nums2.remove(x)
        return rlist